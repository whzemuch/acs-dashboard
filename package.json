  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@turf/centroid": "^7.2.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "csv-parse": "^6.1.0",
